function [Adash,err]=CompletionTest(A,obs,Nrows,Ncols,k)
% COMPLETIONTEST attempt to recover a matrix from sampled entries with MMF
% 
% [Adash,err]=CompletionTest(A,obs,Nrows,Ncols,k)
if nargin<5 k=2; end
[n,m]=size(A);
I=randi(n,obs,1);
J=randi(m,obs,1);
v=zeros(obs,1);
for o=1:obs
    v(o)=A(I(o),J(o));
end
[U,S,V]=MMFcomplete(I,J,v,n,m,Nrows,Ncols,k);
Adash=U*S*V';
%A-Adash
err=norm(A-Adash,'fro')/norm(A,'fro');
fprintf('JacobiError=%f\n',err);
end